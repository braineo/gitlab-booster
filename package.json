  "bugs": {
    "url": "https://github.com/braineo/gitlab-booster/issues"
  },
  "homepage": "https://github.com/braineo/gitlab-booster#readme",
  "devDependencies": {
    "@biomejs/biome": "1.9.3",
    "@eslint/js": "^9.11.1",
    "@types/jquery": "^3.5.31",
    "@types/tampermonkey": "^5.0.3",
    "eslint": "^9.11.1",
    "eslint-plugin-userscripts": "^0.5.5",
    "globals": "^15.9.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.12",
    "vite-plugin-monkey": "^4.0.0"
