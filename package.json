{
  "name": "gitlab-booster",
  "version": "1.4.0",
  "description": "Boost productivity for code reviewers on gitlab",
  "main": "gitlab-booster.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "vite",
